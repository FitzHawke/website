{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": "./",
		"jsx": "react-jsx",
		"types": ["vite/client"],
		"paths": {
			"@assets/*": ["src/assets/*"],
			"@components/*": ["src/components/*"],
			"@data/*": ["src/data/*"],
			"@partials/*": ["src/partials/*"],
			"@pages/*": ["src/pages/*"],
			"@src/*": ["src/*"]
		},
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		]
	}
}
