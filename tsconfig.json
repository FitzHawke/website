{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@data/*": ["src/data/*"],
      "@partials/*": ["src/partials/*"],
      "@components/*": ["src/components/*"],
      "@src/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.astro"]
}
